<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core"
    controllerName="br.com.gestao.fioriappusers358.controller.Lista"
    displayBlock="true"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <content>
            <SemanticPage
                xmlns="sap.f.semantic"
                showFooter="false"
                id="pageLista"
                headerPinnable="true"
                toggleHeaderOnTitleClick="true"
                preserveHeaderStateOnScroll="false"
                titleAreaShrinkRatio="1:1.6:1.6"
            >
                <titleHeading>
                    <m:Title
                        xmlns:m="sap.m"
                        text="{i18n>headingTitle}"
                        id="title0"
                    />
                </titleHeading>
                <headerContent>
                    <fb:FilterBar
                        id="filterbar"
                        persistencyKey="myPersKey"
                        useToolbar="false"
                        search="onSearch"
                        showGoOnFB="true"
                    >
                        <fb:filterGroupItems>
                            <fb:FilterGroupItem
                                xmlns:sap.m="sap.m"
                                label="{i18n>fgitemUserid}"
                                name="Usuario Id"
                                labelTooltip="{i18n>fgitemUseridTooltip}"
                                visibleInFilterBar="true"
                                groupTitle="{i18n>fgitemUseridGroup}"
                                groupName="Principal"
                                id="item0"
                            >
                                <fb:control>
                                    <sap.m:Input
                                        width="100%"
                                        id="usuarioIdInput"
                                        placeholder="{i18n>fgitemUseridplaceholder}"
                                    />
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                xmlns:sap.m="sap.m"
                                label="{i18n>fgitemNomeUsuario}"
                                name="Primeiro Nome"
                                labelTooltip="{i18n>fgitemNomeUsuarioTooltip}"
                                groupTitle="{i18n>fgitemNomeUsuarioGroup}"
                                groupName="Principal"
                                visibleInFilterBar="true"
                                id="item0_1709855403143"
                            >
                                <fb:control>
                                    <sap.m:Input
                                        width="100%"
                                        id="usuarioFirstNameInput"
                                        placeholder="{i18n>fgitemNomeUsuarioplaceholder}"
                                    />
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                xmlns:sap.m="sap.m"
                                id="_IDGenFilterGroupItem1"
                                label="{i18n>fgitemEmail}"
                                name="Email"
                                labelTooltip="{i18n>fgitemEmailTooltip}"
                                groupTitle="{i18n>fgitemEmailGroup}"
                                groupName="Principal"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <sap.m:Input
                                        width="100%"
                                        id="emailInput"
                                        placeholder="{i18n>fgitemEmailplaceholder}"
                                    />
                                </fb:control>
                            </fb:FilterGroupItem>
                        </fb:filterGroupItems>
                    </fb:FilterBar>
                </headerContent>
                <titleMainAction />
                <titleCustomTextActions>
                    <Button
                        xmlns="sap.m"
                        text="{i18n>btnNovo}"
                        id="button0"
                        icon="sap-icon://add"
                        press="onNovoUsuario"
                    />
                </titleCustomTextActions>
                <titleCustomIconActions />
                <content>
                    <sap.m:Table
                        xmlns:sap.m="sap.m"
                        noDataText="{i18n>tbNoData}"
                        id="tableUsuarios"
                        items="{/UsersSet}"
                    >
                        <sap.m:items>
                            <sap.m:ColumnListItem
                                type="Active"
                                id="item0_1709856704796"
                                press="onSelectedItem"
                            >
                                <sap.m:cells>
                                    <sap.ui.core:Icon
                                        xmlns:sap.ui.core="sap.ui.core"
                                        src="sap-icon://account"
                                        size="2rem"
                                        id="icon0"
                                    />
                                    <Text
                                        xmlns="sap.m"
                                        text="{Userid}"
                                        id="text15"
                                    />
                                    <Text
                                        xmlns="sap.m"
                                        text="{Firstname} {Lastname}"
                                        id="text19"
                                    />
                                    <Text
                                        xmlns="sap.m"
                                        text="{Email}"
                                        id="text18"
                                    />
                                    <Text
                                        xmlns="sap.m"
                                        text="{Fone}"
                                        id="text20"
                                    />
                                </sap.m:cells>
                            </sap.m:ColumnListItem>
                        </sap.m:items>
                        <sap.m:columns>
                            <sap.m:Column
                                id="column0"
                                hAlign="Center"
                                width="7em"
                            >
                                <sap.m:header>
                                    <sap.m:Label
                                        text="{i18n>tbcolumnLogo}"
                                        id="label0"
                                    />
                                </sap.m:header>
                            </sap.m:Column>
                            <sap.m:Column
                                id="column1"
                                hAlign="Center"
                                width="12em"
                            >
                                <sap.m:header>
                                    <sap.m:Label
                                        text="{i18n>tbcolumnUserId}"
                                        id="label1"
                                    />
                                </sap.m:header>
                            </sap.m:Column>
                            <sap.m:Column
                                id="column2"
                                width="12em"
                            >
                                <sap.m:header>
                                    <sap.m:Label
                                        text="{i18n>tbcolumnNome}"
                                        id="label2"
                                    />
                                </sap.m:header>
                            </sap.m:Column>
                            <sap.m:Column
                                id="column0_1710006449504"
                                width="15em"
                            >
                                <sap.m:header>
                                    <sap.m:Text
                                        text="{i18n>tbcolumnEmail}"
                                        id="text9"
                                    />
                                </sap.m:header>
                            </sap.m:Column>
                            <sap.m:Column
                                id="column1_1710006456937"
                                width="15em"
                                hAlign="Center"
                            >
                                <sap.m:header>
                                    <sap.m:Text
                                        text="{i18n>tbcolumnFone}"
                                        id="text10"
                                    />
                                </sap.m:header>
                            </sap.m:Column>
                        </sap.m:columns>
                    </sap.m:Table>
                </content>
                <footerCustomActions />
                <titleSnappedContent>
                    <Title
                        xmlns="sap.m"
                        text="{i18n>headingTitleSnappedContent}"
                        id="title0_1709682444437"
                    />
                </titleSnappedContent>
            </SemanticPage>
        </content>
    </Page>
</mvc:View>
