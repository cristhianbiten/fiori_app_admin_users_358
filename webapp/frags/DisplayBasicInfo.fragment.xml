<core:FragmentDefinition
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:forms="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
>
    <m:VBox
        id="vboxBasicInfo"
        class="sapUiSmallMargin"
    >
        <sap.ui.layout.form:SimpleForm
            xmlns:sap.ui.layout.form="sap.ui.layout.form"
            xmlns:sap.ui.core="sap.ui.core"
            id="form0"
            class="sapUxAPObjectPageSubSectionAlignContent"
            editable="false"
            layout="ColumnLayout"
            labelSpanXL="3"
            labelSpanL="3"
            labelSpanM="3"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="4"
            emptySpanL="4"
            emptySpanM="4"
            emptySpanS="0"
            columnsM="2"
            columnsL="3"
            columnsXL="4"
            singleContainerFullSize="false"
        >
            <sap.ui.layout.form:content>
                <!-- Propriedade Nome Usuario -->
                <m:Label
                    id="_IDGenLabel9"
                    text="{i18n>sfNome}"
                />
                <m:Text
                    id="_IDGenText5"
                    text="{Firstname} {Lastname}"
                    class="sapMObjectNumberMedium"
                />

                <!-- Propriedade Email -->
                <m:Label
                    id="_IDGenLabel10"
                    text="{i18n>sfEmail}"
                />
                <m:Text
                    id="_IDGenText6"
                    text="{Email}"
                    class="sapMObjectNumberMedium"
                />

                <!-- Propriedade Fone -->
                <m:Label
                    id="_IDGenLabel11"
                    text="{i18n>sfFone}"
                />
                <m:Text
                    id="_IDGenText7"
                    text="{Fone}"
                    class="sapMObjectNumberMedium"
                />
            </sap.ui.layout.form:content>
        </sap.ui.layout.form:SimpleForm>
    </m:VBox>
</core:FragmentDefinition>
